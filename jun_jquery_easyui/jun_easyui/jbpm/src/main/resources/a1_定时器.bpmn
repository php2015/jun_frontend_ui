  <itemDefinition id="_indexItem" structureRef="Integer" />

  <process processType="Private" isExecutable="true" id="com.sample.bpmn" name="Sample Process" tns:packageName="defaultPackage" >

    <extensionElements>
     <tns:import name="java.util.*" />
    </extensionElements>
    <!-- process variables -->
    <property id="index" itemSubjectRef="_indexItem"/>

    <!-- nodes -->
    <startEvent id="_1" name="StartProcess" />
    <scriptTask id="_2" name="Input content" scriptFormat="http://www.java.com/java" >
      <script>System.out.println("请输入内容");
Scanner scanner = new Scanner(System.in);
int i = scanner.nextInt();
kcontext.setVariable("index",i);
</script>
    </scriptTask>
    <intermediateCatchEvent id="_3" name="Timer" >
      <timerEventDefinition>
        <timeDuration xsi:type="tFormalExpression">5s</timeDuration>
      </timerEventDefinition>
    </intermediateCatchEvent>
    <scriptTask id="_4" name="һ�����ʾ" scriptFormat="http://www.java.com/java" >
      <script>System.out.println("�������������"+index);</script>
    </scriptTask>
    <endEvent id="_5" name="End" >
        <terminateEventDefinition />
    </endEvent>

